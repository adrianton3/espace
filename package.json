{
  "name": "espace",
  "version": "0.0.0",
  "devDependencies": {
    "uglify-es": "^3.1.10"
  },
  "scripts": {
    "uglify": "uglifyjs src/**/*.js > tmp/espace.min.unwrap.js",
    "wrap": "cat tools/header.js tmp/espace.min.unwrap.js tools/footer.js > tmp/espace.min.js",
    "build": "npm run uglify && npm run wrap && rm tmp/espace.min.unwrap.js"
  }
}
