{
  "name": "espace",
  "version": "0.0.0",
  "devDependencies": {
    "rollup": "^4.44.2",
    "uglify-js": "^3.19.3"
  },
  "scripts": {
    "roll": "rollup --config ./tools/rollup.config.mjs",
    "minify": "uglifyjs ./build/espace.mjs --config-file ./tools/uglify.config.json --output ./build/espace.min.mjs",
    "build": "npm run roll && npm run minify"
  }
}
